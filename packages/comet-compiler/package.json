{
  "name": "@stellaria/comet-compiler",
  "version": "0.0.11",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "William JCR (WillishakespeareSKR13)",
  "description": "A compiler to Comet Library",
  "repository": {
    "type": "git",
    "url": "https://github.com/StellariaApp/Comet.git",
    "directory": "packages/comet-compiler"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./assets/*": "./dist/assets/*",
    "./webpack/loader": {
      "types": "./dist/plugins/webpack/loader.d.ts",
      "import": "./dist/plugins/webpack/loader.mjs",
      "require": "./dist/plugins/webpack/loader.js"
    },
    "./webpack/cssLoader": {
      "types": "./dist/plugins/webpack/cssLoader.d.ts",
      "import": "./dist/plugins/webpack/cssLoader.mjs",
      "require": "./dist/plugins/webpack/cssLoader.js"
    },
    "./webpack/plugin": {
      "types": "./dist/plugins/webpack/plugin.d.ts",
      "import": "./dist/plugins/webpack/plugin.mjs",
      "require": "./dist/plugins/webpack/plugin.js"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**/*",
    "dist/assets/**/*",
    "dist/assets/comet.css"
  ],
  "scripts": {
    "build": "tsup --clean && npm run copy",
    "dev": "npm run copy && tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rimraf -rf .turbo && rimraf -rf node_modules && rimraf -rf dist",
    "copy": "copyfiles -f ./src/assets/**/* ./dist/assets"
  },
  "dependencies": {
    "esbuild": "^0.19.2"
  },
  "devDependencies": {
    "typescript-config-stellaria-comet": "*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "next": "^13.4.19",
    "vite": "^4.4.4",
    "webpack": "^5.88.1",
    "eslint": "^7.32.0",
    "eslint-config-stellaria-comet": "*",
    "react": "^18.2.0",
    "tsup": "^5.10.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "next": "^13",
    "vite": "^4",
    "webpack": "^5"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}
