{
  "name": "@stellaria/comet-compiler",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "William JCR (WillishakespeareSKR13)",
  "description": "A compiler to Comet Library",
  "repository": {
    "type": "git",
    "url": "https://github.com/StellariaApp/Comet.git",
    "directory": "packages/comet-compiler"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@stellaria/comet-compiler-rust": "workspace:*",
    "esbuild": "^0.19.2"
  },
  "devDependencies": {
    "typescript-config-stellaria-comet": "*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "next": "^13.4.19",
    "vite": "^4.4.4",
    "webpack": "^5.88.1",
    "eslint": "^7.32.0",
    "eslint-config-stellaria-comet": "*",
    "react": "^18.2.0",
    "tsup": "^5.10.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "next": "^13",
    "vite": "^4",
    "webpack": "^5"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}
