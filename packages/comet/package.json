{
  "name": "@stellaria/comet",
  "version": "0.0.10",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "William JCR (WillishakespeareSKR13)",
  "description": "CSS in JS compiler for Stellaria",
  "repository": {
    "type": "git",
    "url": "https://github.com/StellariaApp/Comet.git",
    "directory": "packages/comet"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./next": {
      "import": "./dist/plugins/next/index.mjs",
      "require": "./dist/plugins/next/index.js",
      "types": "./dist/plugins/next/index.d.ts"
    },
    "./vite": {
      "import": "./dist/plugins/vite/index.mjs",
      "require": "./dist/plugins/vite/index.js",
      "types": "./dist/plugins/vite/index.d.ts"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/plugins/next/index.ts src/plugins/vite/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@stellaria/comet-compiler": "*"
  },
  "devDependencies": {
    "typescript-config-stellaria-comet": "*",
    "eslint": "^7.32.0",
    "eslint-config-stellaria-comet": "*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^17.0.2",
    "tsup": "^5.10.1",
    "typescript": "^5.1.6"
  }
}
