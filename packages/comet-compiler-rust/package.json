{
  "name": "@stellaria/comet-compiler-rust",
  "version": "1.0.0",
  "author": "William JCR (WillishakespeareSKR13)",
  "description": "A Rust compiler for Comet",
  "repository": {
    "type": "git",
    "url": "https://github.com/StellariaApp/Comet.git",
    "directory": "packages/comet-compiler-rust"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/web/comet-compiler-rust.d.ts",
