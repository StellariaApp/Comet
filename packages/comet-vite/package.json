{
  "name": "@stellaria/comet-vite",
  "version": "0.0.4",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "William JCR (WillishakespeareSKR13)",
  "description": "A Vite plugin for Comet",
  "repository": {
    "type": "git",
    "url": "https://github.com/StellariaApp/Comet.git",
    "directory": "packages/comet-vite"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@stellaria/comet": "*",
    "@stellaria/comet-compiler": "*"
  },
  "devDependencies": {
    "typescript-config-stellaria-comet": "*",
    "eslint": "^7.32.0",
    "eslint-config-stellaria-comet": "*",
    "tsup": "^5.10.1",
    "typescript": "^5.1.6"
  }
}
