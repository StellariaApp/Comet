{
  "name": "@stellaria/comet-vite",
  "version": "1.0.0",
  "author": "William JCR (WillishakespeareSKR13)",
  "description": "A Vite plugin for Comet",
  "repository": {
    "type": "git",
    "url": "https://github.com/StellariaApp/Comet.git",
    "directory": "packages/comet-vite"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --dts --external react --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "typescript-config-stellaria-comet": "*",
    "@stellaria/comet": "*",
    "eslint": "^7.32.0",
    "eslint-config-stellaria-comet": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
